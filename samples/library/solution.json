{
    "Projects": [
        {
            "Name": "sample",
            "Source": "sample/src",
            "Bin": "sample/bin",
            "IgnorePathsOnTranslate": [
                "libs",
                "node_modules",
                "files/assets/js"
            ],
            "Type": "web",
            "MainFile": "main.js",
            "References": [
                {
                    "Project": "sample.DAL",
                    "Out": "./libs/"
                }
            ]
        },
        {
            "Name": "sample.DAL",
            "Source": "sample.DAL/src",
            "Bin": "sample.DAL/bin",
            "IgnorePathsOnTranslate": [
                "libs",
                "node_modules"
            ],
            "Type": "library",
            "MainFile": "sample.DAL.js"
        }
    ],
    "Options": {
        "translateModule": true,
        "translateExports": true,
        "translateImports": true,
        "translateClass": true,
        "translateStringTemplates": true,
        "translateArrowFunctions": true,
        "translateLetAndConst": true,
        "translateFunctions": true,
        "translateTypedArrays": true,
        "translateAsyncFunctions": true,
        "translateCodeAfterImport": false
    }
}